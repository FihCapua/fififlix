generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Movie {
  id            Int      @id @default(autoincrement())
  title         String
  coverImage    String
  releaseDate   DateTime
  rating        Int
  director      String
  writer        String
  mainActors    String
  nationality   String
  comment       String
  createdAt     DateTime @default(now())
}